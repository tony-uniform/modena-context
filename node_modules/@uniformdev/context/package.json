{
  "name": "@uniformdev/context",
  "version": "16.1.0",
  "description": "Uniform Context core package",
  "license": "SEE LICENSE IN LICENSE.txt",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "import": {
        "node": "./dist/index.mjs",
        "default": "./dist/index.esm.js"
      },
      "require": "./dist/index.js"
    },
    "./cli": {
      "import": "./dist/cli/cli.mjs",
      "require": "./dist/cli/cli.js"
    },
    "./api": {
      "import": "./dist/api/api.mjs",
      "require": "./dist/api/api.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "cli": [
        "./dist/cli/cli.d.ts"
      ],
      "api": [
        "./dist/api/api.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup --minify",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "jest --maxWorkers=1",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "update-openapi": "node ./update-openapi.cjs",
    "benchmark:build": "tsup src/storage/__benchmarks__/storage.benchmark.ts",
    "benchmark:run": "node ./dist/storage.benchmark.js"
  },
  "devDependencies": {
    "@types/js-cookie": "3.0.2",
    "@types/yargs": "17.0.10",
    "@uniformdev/cli": "^16.1.0",
    "benny": "3.7.1",
    "yargs": "17.5.1"
  },
  "dependencies": {
    "dequal": "^2.0.2",
    "isomorphic-unfetch": "^3.1.0",
    "js-cookie": "3.0.1",
    "mitt": "^3.0.0",
    "p-limit": "^3.1.0",
    "rfdc": "^1.3.0"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "18a24d68a80d079bfdf74b52ccf352fc2f8a2821"
}
