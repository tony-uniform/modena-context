{
  "name": "@uniformdev/canvas",
  "version": "16.1.0",
  "description": "Common functionality and types for Uniform Canvas",
  "license": "SEE LICENSE IN LICENSE.txt",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "import": {
        "node": "./dist/index.mjs",
        "default": "./dist/index.esm.js"
      },
      "require": "./dist/index.js"
    },
    "./cli": {
      "import": "./dist/cli/cli.mjs",
      "require": "./dist/cli/cli.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "cli": [
        "./dist/cli/cli.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup --minify",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "jest --maxWorkers=1",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\""
  },
  "devDependencies": {
    "@types/retry": "0.12.1",
    "@types/yargs": "17.0.10",
    "@uniformdev/cli": "^16.1.0",
    "pusher-js": "7.0.6",
    "yargs": "17.5.1"
  },
  "dependencies": {
    "@uniformdev/context": "^16.1.0",
    "isomorphic-unfetch": "^3.1.0",
    "p-limit": "^3.1.0",
    "p-retry": "^4.6.2",
    "p-throttle": "^4.1.1"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "18a24d68a80d079bfdf74b52ccf352fc2f8a2821"
}
