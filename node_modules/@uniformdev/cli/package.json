{
  "name": "@uniformdev/cli",
  "version": "16.1.0",
  "description": "Uniform command line interface tool",
  "license": "SEE LICENSE IN LICENSE.txt",
  "main": "./cli.js",
  "exports": {
    ".": "./cli.js",
    "./sync": {
      "import": "./dist/sync/index.mjs",
      "require": "./dist/sync/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "sync": [
        "./dist/sync/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "uniform": "node ./cli.js",
    "build": "tsup --minify",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "jest --maxWorkers=1 --passWithNoTests",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\""
  },
  "dependencies": {
    "diff": "^5.0.0",
    "dotenv": "^16.0.1",
    "js-yaml": "^4.1.0",
    "lodash.isequalwith": "^4.4.0",
    "node-fetch": "^2.6.7",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/diff": "5.0.2",
    "@types/js-yaml": "4.0.5",
    "@types/lodash.isequalwith": "4.4.7",
    "@types/node-fetch": "2.6.1",
    "@types/yargs": "17.0.10"
  },
  "bin": {
    "uniform": "./cli.js"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "18a24d68a80d079bfdf74b52ccf352fc2f8a2821"
}
